{
  "manifest_version": 3,
  "name": "phpMyAdmin HotStuff",
  "version": "1.0",
  "description": "Adds a hotkey to navigate to phpMyAdmin",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": [
        "*://mysql.spend.cloud/index.php*",
        "*://mysql.dev.spend.cloud/*",
        "*://*/phpmyadmin/*",
        "*://*/pma/*"
      ],
      "js": ["content.js"]
    }
  ]
}